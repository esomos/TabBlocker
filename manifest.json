{
    "manifest_version": 3,
    "name": "Tab Blocker",
    "version": "1.0",
    "description": "Blocks new tabs based on the website that created them.",
    "permissions": ["tabs", "notifications", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  